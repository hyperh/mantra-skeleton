{
  "name": "mantra-skeleton",
  "private": true,
  "scripts": {
    "start": "meteor run",
    "lint": "eslint ./lib ./client ./server --ext .js --ext .jsx",
    "lintfix": "npm run lint -- --fix",
    "testonly": "mocha client/**/tests/**/*.js --compilers js:babel-core/register",
    "test": "npm run lint && npm run testonly",
    "test-watch": "npm run testonly -- --watch --watch-extensions js,jsx",
    "storybook": "start-storybook -p 9001"
  },
  "devDependencies": {
    "@kadira/storybook": "^1.1.0",
    "babel-core": "6.x.x",
    "babel-plugin-react-require": "2.x.x",
    "babel-polyfill": "6.x.x",
    "babel-preset-es2015": "6.x.x",
    "babel-preset-react": "6.x.x",
    "babel-preset-stage-2": "6.x.x",
    "babel-root-slash-import": "1.x.x",
    "chai": "3.x.x",
    "enzyme": "^2.3.0",
    "eslint": "^2.13.1",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-import-resolver-meteor": "^0.2.4",
    "eslint-plugin-import": "^1.10.0",
    "eslint-plugin-jsx-a11y": "^1.5.3",
    "eslint-plugin-meteor": "^3.6.0",
    "eslint-plugin-react": "^5.2.2",
    "mocha": "2.x.x",
    "react-addons-test-utils": "^15.2.0",
    "sinon": "1.17.x"
  },
  "dependencies": {
    "mantra-core": "^1.4.0",
    "meteor-auth": "^0.2.0",
    "meteor-node-stubs": "~0.2.0",
    "ramda": "^0.21.0",
    "react": "^15.2.0",
    "react-dom": "^15.2.0",
    "react-mounter": "^1.0.0"
  }
}
